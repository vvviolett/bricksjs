{
  "name": "bricksjs",
  "version": "0.0.1",
  "description": "A javascript utility library",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./types/src/browser.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./types/src/browser.d.ts",
        "default": "./dist/index.es.js"
      },
      "require": {
        "types": "./types/src/node.d.ts",
        "default": "./dist/index.cjs.js"
      }
    }
  },
  "scripts": {
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm",
    "build": "npm-run-all build:*",
    "build:tspc": "tspc -p tsconfig.declare.json",
    "build:dist": "rollup --config rollup.config.ts --configPlugin typescript",
    "docs:dev": "dumi dev",
    "docs:build": "dumi build",
    "test": "jest",
    "clean": "rimraf dist/ types/",
    "eslint": "eslint **/*.{ts,js}",
    "lint-staged": "npx lint-staged"
  },
  "keywords": [
    "lodash",
    "lodash-es",
    "bricksjs",
    "szchason"
  ],
  "author": "szchason",
  "bugs": {
    "url": "https://github.com/szchason/bricksjs/issues"
  },
  "homepage": "https://github.com/szchason/bricksjs#readme",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/plugin-transform-runtime": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@jest/globals": "^29.6.1",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dumi": "^2.2.1",
    "eslint": "^8.45.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.3",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "lint-staged": "^13.2.3",
    "npm-run-all": "^4.1.5",
    "only-allow": "^1.1.1",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.26.2",
    "ts-jest": "^29.1.1",
    "ts-patch": "^3.0.1",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6",
    "typescript-transform-paths": "^3.4.6"
  },
  "lint-staged": {
    "*.{ts,js}": "npx eslint"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.22.6",
    "core-js": "^3.31.1"
  }
}
